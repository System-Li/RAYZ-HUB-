--// UI erstellen
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "KeySystem"

local mainFrame = Instance.new("Frame", gui)
mainFrame.Size = UDim2.new(0, 400, 0, 250)
mainFrame.Position = UDim2.new(0.5, -200, 0.5, -125)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BorderSizePixel = 0
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.ClipsDescendants = true
mainFrame.BackgroundTransparency = 1
mainFrame.Visible = false

local corner = Instance.new("UICorner", mainFrame)
corner.CornerRadius = UDim.new(0, 20)

local title = Instance.new("TextLabel", mainFrame)
title.Size = UDim2.new(1, 0, 0, 50)
title.BackgroundTransparency = 1
title.Text = "üîë RAYZ HUB Key System"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 22

local textbox = Instance.new("TextBox", mainFrame)
textbox.Size = UDim2.new(0.8, 0, 0, 40)
textbox.Position = UDim2.new(0.1, 0, 0.4, 0)
textbox.PlaceholderText = "Gib deinen Key ein..."
textbox.Text = ""
textbox.TextSize = 18
textbox.Font = Enum.Font.Gotham
textbox.TextColor3 = Color3.fromRGB(0, 0, 0)
textbox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
textbox.ClearTextOnFocus = false
Instance.new("UICorner", textbox).CornerRadius = UDim.new(0, 10)

local button = Instance.new("TextButton", mainFrame)
button.Size = UDim2.new(0.5, 0, 0, 40)
button.Position = UDim2.new(0.25, 0, 0.7, 0)
button.Text = "‚úÖ CheckKey"
button.Font = Enum.Font.GothamBold
button.TextSize = 18
button.BackgroundColor3 = Color3.fromRGB(0, 170, 127)
button.TextColor3 = Color3.fromRGB(255, 255, 255)
Instance.new("UICorner", button).CornerRadius = UDim.new(0, 10)

--// Ein-/Ausblenden mit Animation
local function openUI()
    mainFrame.Visible = true
    TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()
end

local function closeUI(callback)
    local tween = TweenService:Create(mainFrame, TweenInfo.new(0.5), {BackgroundTransparency = 1})
    tween:Play()
    tween.Completed:Connect(function()
        mainFrame.Visible = false
        if callback then callback() end
    end)
end

--// Keypr√ºfung und Scriptausf√ºhrung
button.MouseButton1Click:Connect(function()
    local key = textbox.Text
    if validKeys[key] then
        closeUI(function()
            loadstring(game:HttpGet("https://pastebin.com/raw/x8Jc0JmP"))()
        end)
    else
        textbox.Text = ""
        textbox.PlaceholderText = "‚ùå Ung√ºltiger Key!"
        textbox.PlaceholderColor3 = Color3.fromRGB(255, 0, 0)
    end
end)

-- UI √∂ffnen
wait(1)
openUI()
